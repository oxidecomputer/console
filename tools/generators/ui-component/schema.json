{
  "cli": "nx",
  "id": "react-component",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Component name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "skipStories": {
      "type": "boolean",
      "description": "Don't create a story for this component",
      "default": false
    }
  },
  "required": ["name"]
}
