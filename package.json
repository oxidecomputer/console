    "downshift": "^6.1.2",
    "filesize": "^8.0.7",
    "lodash.throttle": "^4.1.1",
    "match-sorter": "^6.3.1",
    "mousetrap": "^1.6.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-aria": "^3.22.0",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^7.43.2",
    "react-is": "^17.0.2",
    "react-merge-refs": "^2.0.1",
    "react-router-dom": "^6.8.1",
    "react-stately": "^3.20.0",
    "recharts": "^2.4.3",
    "simplebar-react": "^2.4.3",
    "tiny-invariant": "^1.2.0",
