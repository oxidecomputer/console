{
  "name": "oxide",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "start:msw": "MSW=1 vite",
    "build": "vite build",
    "build-for-nexus": "API_URL='' vite build",
    "build:themes": "./tools/build_themes.sh",
    "ci": "yarn tsc && yarn lint && yarn test run && yarn e2e",
    "test": "vitest",
    "e2e": "playwright test",
    "lint": "eslint --ext .js,.ts,.tsx,.json .",
    "fmt": "prettier --write .",
    "gen": "plop",
    "gen-api": "./tools/generate_api_client.sh",
    "storybook": "start-storybook --ci -c libs/ui/.storybook -p 4400",
    "storybook:clean": "rm -rf node_modules/.cache",
    "build:storybook": "build-storybook -c libs/ui/.storybook -o dist/storybook/ui",
    "//": "Required due to jscodeshift shipping with windows line endings starting in v0.12. See https://github.com/facebook/jscodeshift/issues/424.",
    "jscodeshift": "node node_modules/.bin/jscodeshift",
    "unused-exports": "ts-unused-exports tsconfig.json --ignoreFiles=\\.stories --excludePathsFromReport=\"libs/api/index.ts;libs/ui/index.ts\"",
    "chromatic": "chromatic --skip '@(renovate/**|dependabot/**)'",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "private": true,
