    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "semver": "^7.3.8",
    "style-dictionary": "^3.1.1",
    "tailwindcss": "^3.2.7",
    "type-fest": "^2.17.0",
