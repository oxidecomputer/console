{
  "name": "oxide",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "start:msw": "MSW=1 vite",
    "dev": "MSW=1 vite",
    "start:mock-api": "node -r esbuild-register ./tools/start_mock_api.ts",
    "build": "vite build",
    "build-for-nexus": "API_URL='' vite build",
    "ci": "npm run tsc && npm run lint && npm run test run && npm run e2ec",
    "tsc": "tsc",
    "test": "vitest",
    "e2e": "playwright test && playwright test -c playwright.config.smoke.ts",
    "e2ec": "npm run e2ecb && npm run e2ecs",
    "e2ecb": "playwright test --project=chrome",
    "e2ecs": "playwright test --project=chrome -c playwright.config.smoke.ts",
    "lint": "eslint --ext .js,.ts,.tsx,.json .",
    "fmt": "prettier --cache --write . && npm run lint -- --fix",
    "gen-api": "./tools/generate_api_client.sh",
    "storybook": "IS_LADLE=1 ladle serve",
    "build:storybook": "IS_LADLE=1 ladle build",
    "//": "Required due to jscodeshift shipping with windows line endings starting in v0.12. See https://github.com/facebook/jscodeshift/issues/424.",
    "jscodeshift": "node node_modules/.bin/jscodeshift",
    "unused-exports": "ts-unused-exports tsconfig.json --ignoreFiles=\\.stories --excludePathsFromReport=\"libs/api/index.ts;libs/ui/index.ts\"",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.10.3",
    "@oxide/design-system": "^0.8.0",
