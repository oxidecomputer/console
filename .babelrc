{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": "3.11",
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["styled-components", { "pure": true, "ssr": true }],
    "@babel/plugin-proposal-class-properties"
  ]
}
