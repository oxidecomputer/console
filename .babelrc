{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    ["styled-components", { "pure": true, "ssr": true }],
    "@babel/plugin-proposal-class-properties"
  ]
}
