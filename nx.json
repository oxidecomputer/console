{
  "npmScope": "oxide",
  "affected": { "defaultBase": "origin/main" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "web-console": { "tags": [] },
    "web-console-e2e": { "tags": [], "implicitDependencies": ["web-console"] },
    "ui": { "tags": [] },
    "theme": { "tags": [] },
    "backend-types": { "tags": [] }
  }
}
